{
    "pages": [
        {
            "path": "pages/index",
            "style": {
                "navigationBarTitleText": "首页"
            }
        }
    ],
    "subPackages": [
        {
            "root": "packages",
            "pages": [
                {
                    "path": "index/museumList/index",
                    "style": {
                        "navigationBarTitleText": "文物清单",
                        "enablePullDownRefresh": false
                    }
                },
                {
                    "path": "index/museumDetail/index",
                    "style": {
                        "navigationBarTitleText": "文物详情",
                        "enablePullDownRefresh": false
                    }
                }
            ]
        }
    ],
    "preloadRule": {
        "pages/index/index": {
            "network": "wifi",
            "packages": ["__APP__"]
        },
        "pages/my/index": {
            "network": "wifi",
            "packages": ["__APP__"]
        }
    },
    "globalStyle": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "即时租赁",
        "navigationBarBackgroundColor": "#F8F8F8",
        "backgroundColor": "#F8F8F8"
    }
}
